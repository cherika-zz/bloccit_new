<h1><%= @question.title %></h1>
<p><%= @question.body %></p>
<div class="row">
  <div class="col-md-8">
    <%= link_to "Edit Question", edit_question_path(@question), class: 'button btn-success' %>
    <%= link_to "Delete Question", question_path(@question), method: :delete, class: 'button btn-success', data: {confirm: "Are you sure you want to delete this question?"} %>
  </div>
</div>
